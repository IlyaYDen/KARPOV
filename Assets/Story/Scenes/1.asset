    actionsAfter: []
    music: {fileID: 0}
    sound: {fileID: 0}
    voice: {fileID: 8300000, guid: 628f3feac0ad55844a25643ba89e0739, type: 3}
  - text: "\u041E\u043D \u0432\u0437\u044F\u043B \u043C\u043E\u044E \u0440\u0443\u043A\u0443
      \u0438 \u043F\u043E\u043A\u0430\u0437\u0430\u043B, \u0433\u0434\u0435 \u043E\u043D\u0430
      \u043D\u0430\u0445\u043E\u0434\u0438\u0442\u0441\u044F. \u041B\u0438\u043D\u0438\u044F
      \u043A\u0430\u043A \u043B\u0438\u043D\u0438\u044F, \u043D\u0438\u0447\u0435\u0433\u043E
      \u043E\u0441\u043E\u0431\u0435\u043D\u043D\u043E\u0433\u043E."
    speaker: {fileID: 11400000, guid: 89f9f1ce7f0ee8c468ac5133634813b4, type: 2}
    showName: 0
    actions:
    - speaker: {fileID: 11400000, guid: eddfe009b49d3484d8dfb5498d6a2f17, type: 2}
      spriteIndex: 0
      actionType: 0
      coords: {x: -180, y: -520}
      size: 1
      moveSpeed: 0
    actionsAfter: []
    music: {fileID: 0}
    sound: {fileID: 0}
    voice: {fileID: 0}
  - text: "\u0422\u043E\u043D\u043A\u0430\u044F \u043D\u0438\u0442\u044C \u0442\u044F\u043D\u0435\u0442\u0441\u044F
      \u0432\u0434\u043E\u043B\u044C \u0432\u0441\u0435\u0439 \u0442\u044B\u043B\u044C\u043D\u043E\u0439
      \u0441\u0442\u043E\u0440\u043E\u043D\u044B \u043B\u0430\u0434\u043E\u043D\u0438
      \u0438 \u0437\u0430\u043A\u0430\u043D\u0447\u0438\u0432\u0430\u0435\u0442\u0441\u044F
      \u0443 \u043E\u0441\u043D\u043E\u0432\u0430\u043D\u0438\u044F \u043A\u0438\u0441\u0442\u0438."
    speaker: {fileID: 11400000, guid: 89f9f1ce7f0ee8c468ac5133634813b4, type: 2}
