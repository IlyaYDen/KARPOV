      moveSpeed: 0
    actionsAfter: []
    music: {fileID: 0}
    sound: {fileID: 0}
    voice: {fileID: 8300000, guid: 6d4659b707da662459e56a1666adfde7, type: 3}
  - text: "- \u0414\u0435\u0434\u0443\u043B\u044C, \u0430 \u0447\u0442\u043E \u0442\u043E\u0433\u0434\u0430
      \u043F\u0440\u043E\u0438\u0437\u043E\u0448\u043B\u043E?"
    speaker: {fileID: 11400000, guid: 755db819ff1a2ea4aa12594b5b3997b8, type: 2}
    showName: 1
